{
  "title": "TEST SITE",
  "link": "http://mysite.com",
  "pages": [
    {
      "path": "/",
      "template": "index.pug",
      "file": "index.json",
      "rss": {
        "filename": "index.rss",
        "template": "rss.pug"
      },
      "deps": [
        {
          "path": "/blog/",
          "template": "blog/list.pug",
          "pagination": 2,
          "deps": [
            {
              "path": "/:filename/",
              "dir": "blog",
              "template": "blog/item.pug"
            }
          ]
        }
      ]
    },
    {
      "path": "/about/",
      "template": "about.pug",
      "file": "about.json"
    }
  ]
}