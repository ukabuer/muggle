{
  "title": "TEST SITE",
  "link": "http://mysite.com",
  "pages": [
    {
      "path": "/",
      "template": "index.pug",
      "file": "index.json",
      "deps": ["/blog/:filename/"]
    },
    {
      "path": "/index.rss",
      "template": "rss.pug",
      "deps": ["/blog/:filename/"]
    },
    {
      "path": "/blog/",
      "template": "blog/list.pug",
      "deps": [{
        "path": "/blog/:filename/",
        "pagination": 2
      }]
    },
    {
      "path": "/blog/:filename/",
      "template": "blog/item.pug",
      "dir": "blog"
    },
    {
      "path": "/about/",
      "template": "about.pug",
      "file": "about.json"
    }
  ]
}