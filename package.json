{
  "name": "muggle",
  "version": "0.8.10",
  "description": "",
  "bin": {
    "muggle": "dist/cjs/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "build": "concurrently npm:build:cjs npm:build:esm",
    "watch:cjs": "tsc -w --module commonjs --outDir dist/cjs",
    "watch:esm": "tsc -w --module esnext --outDir dist/esm",
    "watch": "concurrently npm:watch:cjs npm:watch:esm",
    "test:lint": "eslint . --max-warnings=0",
    "test:format": "prettier --check .",
    "test": "concurrently npm:test:lint npm:test:format"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@barba/core": "^2.9.7",
    "cac": "^6.7.3",
    "concurrently": "^7.3.0",
    "fs-extra": "^10.0.0",
    "glob": "^8.0.3",
    "polka": "^1.0.0-next.15",
    "preact-render-to-string": "^5.2.1",
    "sirv": "^1.0.12",
    "typescript": "^4.2.4",
    "vite": "^3.0.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.12",
    "@types/glob": "^7.2.0",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.7.1"
  },
  "peerDependencies": {
    "preact": "^10.10.6"
  }
}
